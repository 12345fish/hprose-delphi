{$IMAGEBASE $400000}
{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$IMPLICITBUILD ON}
{$DESCRIPTION 'Hprose for Delphi with Indy10.5'}

requires
  rtl,
  IndyCore,
  IndyProtocols,
  IndySystem;

contains
  HproseClient in 'HproseClient.pas',
  HproseCommon in 'HproseCommon.pas',
  HproseIO in 'HproseIO.pas',
  HproseIdHttpClient in 'Indy10_5\HproseIdHttpClient.pas';

end.
